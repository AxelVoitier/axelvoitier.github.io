---
layout: default
---
= render '_includes/article.haml', {:article => page}

.publication
  .shr-bookmarks.shr-bookmarks-bg-knowledge
    %ul.socials
      = get_publication_socials page, ['linkedin', 'googlereader', 'facebook', 'twitter', 'friendfeed', 'digg', 'reddit', 'delicious', 'wikio', 'mail']
    %div{ :style => "clear:both" }

  #disqus_thread
  -#%script{ :type => "text/javascript"}
  :javascript
    var disqus_identifier='#{page.disqus_identifier}';
  %noscript
    %p
      Please enable JavaScript to view the
      = link_to "comments powered by Disqus.", "http://disqus.com/?ref_noscript=robotribune"
